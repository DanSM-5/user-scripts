{
  "files.associations": {
    // ".env*": "dotenv", // messes up syntax highlight
    "*.cfg": "ini",
    "*.cnf": "ini",
    "*.conf": "ini",
    "*.config": "ini",
    "*.ini": "ini",
    "*.yaml": "yaml",
    "*.yml": "yaml"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "default"
  },
  // "powershell.powerShellAdditionalExePaths": [
  //     {
  //         "enable": true,
  //         "exePath": "C:\\Users\\Eduardo-Sanchez\\AppData\\Local\\Microsoft\\WindowsApps\\pwsh.exe",
  //         "versionName": "7.2.1"
  //     }
  // ],
  "omnisharp.useModernNet": true,
  "omnisharp.dotnetPath": "C:\\Program Files\\dotnet",
  
  // VSCode Telemetry
  "telemetry.telemetryLevel": "off",
  "telemetry.feedback.enabled": false,
  "editor.experimental.treeSitterTelemetry": false,

  // LSP Telemetry
  "docker.lsp.telemetry": "off",
  "omnisharp.enableTelemetry": false,
  "powershell.telemetry.enable": false,
  "pylance.telemetry": false,
  "python.telemetry.enable": false,
  "debugpy.telemetry.enable": false,
  "lua.telemetry.enable": false,

  // "terminal.external.windowsExec": "wt -p Ubuntu --title \"VSCode debugger\" Ubuntu",
  "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
  "markdown-preview-enhanced.mermaidTheme": "dark",
  "security.workspace.trust.untrustedFiles": "open",
  "debug.javascript.autoAttachFilter": "disabled",
  "editor.fontFamily": "CaskaydiaCove Nerd Font",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "prettier.singleQuote": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.bracketPairColorization.enabled": true,
  "editor.stickyScroll.enabled": true,
  "scm.alwaysShowActions": true,
  "vsicons.dontShowNewVersionMessage": true,
  "git.mergeEditor": false,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.shellIntegration.suggestEnabled": true,
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font",
  "terminal.integrated.fontLigatures": true,
  "emmet.useInlineCompletions": true,
  "workbench.colorCustomizations": {
    "statusBar.background" : "#833ea6",
    "statusBar.noFolderBackground" : "#212121",
    "statusBar.debuggingBackground": "#ee14f2b3"
  },
  "editor.accessibilitySupport": "off",
  "vscode-neovim.neovimInitVimPaths.linux": "~/.config/vscode-nvim/init.lua",
  "vscode-neovim.neovimInitVimPaths.win32": "~/.config/vscode-nvim/init.lua",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/vscode-nvim/init.lua",
  // "vscode-neovim.useWSL": true,
  "vscode-neovim.neovimExecutablePaths.linux": "~/.config/vscode-nvim/nvimw",
  "vscode-neovim.neovimExecutablePaths.win32": "nvim",
  "vscode-neovim.NVIM_APPNAME": "vscode-nvim",
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape"
    }
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.lineNumbers": "relative",
  "diffEditor.diffAlgorithm": "advanced",
  "cSpell.userWords": [
    "vsicons"
  ],
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Git Zsh": {
      "path": "C:\\Program Files\\Git\\usr\\bin\\env.exe",
      "args": [
        "MSYS=enable_pcon",
        "MSYSTEM=MINGW64",
        "enable_pcon=1",
        "/usr/bin/zsh",
        "-li"
      ],
      "icon": "terminal-bash",
      "overrideName": true
    }
  },
  // "terminal.integrated.defaultProfile.linux": "bash",
  "git.defaultBranchName": "master",
  "window.zoomLevel": 3,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "files.eol": "\n",
  "workbench.sideBar.location": "right",
  "search.useIgnoreFiles": false,
  "search.followSymlinks": false,
  "search.quickOpen.history.filterSortOrder": "recency",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "phase",
  "editor.inlayHints.enabled": "on",
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": false,
  "javascript.inlayHints.variableTypes.enabled": false,
  "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": false,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.inlayHints.enabled": "on"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.inlayHints.enabled": "on"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.inlayHints.enabled": "on"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.chat.completionContext.typescript.mode": "on",
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "github.copilot.chat.edits.temporalContext.enabled": true,
  "github.copilot.chat.languageContext.inline.typescript.enabled": true,
  "github.copilot.chat.languageContext.typescript.enabled": true,
  "github.copilot.chat.localeOverride": "en",
  "github.copilot.selectedCompletionModel": "",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "dotenv": false,
    "ini": false,
    "yaml": false,
    "json": false
  },
  "github.copilot.advanced": {
    "webSearch": false
  }
}
